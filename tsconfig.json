{
   "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "outDir": "./dist",
      "ignoreDeprecations": "5.0",
   },
   "exclude": ["src/**/*.js"]
}
{
   "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": true,
      "outDir": "./dist",
      "verbatimModuleSyntax": true
   },
   "extends": "./.svelte-kit/tsconfig.json",
   "exclude": ["src/**/*.js", "node_modules"]
}